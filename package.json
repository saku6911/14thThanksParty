{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "run-s css:typed:clean css:typed",
    "css:typed": "tcm -e -p 'src/**/**/*.module.scss'",
		"css:typed:clean": "find src -name '*.module.scss.d.ts' -type f -delete && find src -type d -empty -delete",
		"css:typed:watch": "tcm -e -w -p 'src/**/**/*.module.scss'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "react-router-dom": "^6.23.1",
    "sass": "^1.77.6"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "npm-run-all": "^4.1.5",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5"
  }
}
